{
  "name": "constructum-api",
  "version": "1.0.0",
  "description": "server side",
  "main": "index.js",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "ts-build": "rimraf ./build && npx tsc",
    "dev": "cross-env NODE_ENV=development npx nodemon",
    "start": "rimraf ./build && npm run ts-build && node build/index.js",
    "lint": "npx eslint ./src",
    "format": "npx eslint ./src --fix",
    "pkgi": "npm i",
    "pkgup": "npm update"
  },
  "author": "letnull19a",
  "license": "ISC",
  "dependencies": {
    "@tsed/logger": "^6.5.0",
    "@types/bcrypt": "^5.0.0",
    "@types/constructum-interfaces": "file:../packages/@types/constructum-interfaces",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.2",
    "constructum-interfaces": "file:../packages/constructum-interfaces",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.1",
    "redis": "^4.6.6",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "husky": "^8.0.3",
    "nodemon": "^1.14.9",
    "prettier": "^2.8.8",
    "url": "^0.11.0"
  }
